{
  "grammars": [
    {
      "name": "blueprint",
      "camelcase": "Blueprint",
      "scope": "source.blueprint",
      "path": ".",
      "file-types": ["bp"],
      "injection-regex": "^blueprint$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for Blueprint DSL",
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-blueprint"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
